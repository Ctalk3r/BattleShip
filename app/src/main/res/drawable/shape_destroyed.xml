<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="-2dp" android:right="-2dp" android:top="-2dp" android:bottom="-2dp">
        <shape
            android:shape="rectangle">
            <solid android:color="#f9edc9"/>
            <stroke android:width="3dp" android:color="#f9393e"/>
        </shape>
    </item>
    <item android:left="-7dp" android:right="-7dp" android:top="-7dp" android:bottom="-7dp">
        <rotate
            android:fromDegrees="45"
            android:toDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke android:width="2dp" android:color="#f9393e"/>
            </shape>
        </rotate>
    </item>
    <item android:left="-7dp" android:right="-7dp" android:top="-7dp" android:bottom="-7dp">
        <rotate
            android:fromDegrees="135"
            android:toDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape
                android:shape="line">
                <stroke android:width="2dp" android:color="#f9393e"/>
            </shape>
        </rotate>
    </item>
</layer-list>